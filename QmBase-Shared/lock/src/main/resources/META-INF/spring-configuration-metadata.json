{
  "groups": [
    {
      "name": "qm.base.lock.distributed",
      "type": "com.qm.base.shared.lock.distribute.DistributedLockProperties",
      "sourceType": "com.qm.base.shared.lock.distribute.DistributedLockProperties",
      "description": "General distributed lock settings"
    },
    {
      "name": "qm.base.lock.zookeeper",
      "type": "com.qm.base.shared.lock.distribute.zookeeper.ZookeeperProperties",
      "sourceType": "com.qm.base.shared.lock.distribute.zookeeper.ZookeeperProperties"
    }
  ],
  "properties": [
    {
      "name": "qm.base.lock.distributed.type",
